{
	"title": "login",
	"type": "object",
	"properties": {
		"username": {
			"type": "string",
			"pattern": "[a-z0-9!#$%&'*+/=?^_`{|}~-]+(?:\\.[a-z0-9!#$%&'*+/=?^_`{|}~-]+)*@(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\\.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?"
		},
		"password": {
			"type": "string",
			"minLength":4,
			"maxLength":64
		}
	},
	"additionalProperties": false,
	"required": ["username","password"]
}